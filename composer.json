{
    "name": "cakephp/app",
    "description": "CakePHP skeleton app",
    "homepage": "http://cakephp.org",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "cakephp/cakephp": "~3.2",
        "mobiledetect/mobiledetectlib": "2.*",
        "cakephp/migrations": "~1.0",
        "cakephp/plugin-installer": "*",
        "phpunit/phpunit": "^5.3",
        "cakephp/cakephp-codesniffer": "^2.0",
        "symfony/event-dispatcher": "^3.1",
        "alaxos/cakephp3-bootstrap-theme": "^0.9.3",
        "cakedc/users": "^3.2",
        "josegonzalez/cakephp-upload": "^3.4",
        "ircmaxell/random-lib": "^1.1",
        "Cewi/Excel": "dev-master",
        "davidyell/proffer": "~0.6"
    },
    "require-dev": {
        "psy/psysh": "@stable",
        "cakephp/debug_kit": "~3.2",
        "cakephp/bake": "~1.1"
    },
    "suggest": {
        "phpunit/phpunit": "Allows automated tests to be run without system-wide install.",
        "cakephp/cakephp-codesniffer": "Allows to check the code against the coding standards used in CakePHP."
    },
    "autoload": {
        "psr-4": {
            "App\\": "src",
            "SavioboscoSubscription\\": "./plugins/SavioboscoSubscription/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "SavioboscoSubscription\\Test\\": "./plugins/SavioboscoSubscription/tests"
        }
    },
    "scripts": {
        "post-install-cmd": "App\\Console\\Installer::postInstall",
        "post-create-project-cmd": "App\\Console\\Installer::postInstall",
        "post-autoload-dump": "Cake\\Composer\\Installer\\PluginInstaller::postAutoloadDump"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/cewi/excel"
        }
    ]
}
